@model GuildCars.Models.Models.Queries.InventoryReportsNewUsedQueryModel

@{
    ViewBag.Title = "Inventory";
}
<h2>Inventory Report</h2>

<table class="table table-bordered table-hover" title="New Vehicles">
    <caption><b>New Vehicles</b></caption>
    <thead class="thead-dark">
        <tr>
            <th scope="col">Year</th>
            <th scope="col">Make</th>
            <th scope="col">Model</th>
            <th scope="col">Count</th>
            <th scope="col">Stock Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model.NewInventory)
        {
            @Html.Raw("<tr><td>")
            @Html.DisplayFor(m => i.Year)
            @Html.Raw("</td><td>")
            @Html.DisplayFor(m => i.Make)
            @Html.Raw("</td><td>")
            @Html.DisplayFor(m => i.Model)
            @Html.Raw("</td><td>")
            @Html.DisplayFor(m => i.Count)
            @Html.Raw("</td><td>")
            @Html.DisplayFor(m => i.StockValue)
            @Html.Raw("</td></tr>")
        }
        </tbody>
</table>
<br /><br />
<table class="table table-bordered table-hover" title="Used Vehicles">
    <caption><b>Used Vehicles</b></caption>
    <thead class="thead-dark">
        <tr>
            <th scope="col">Year</th>
            <th scope="col">Make</th>
            <th scope="col">Model</th>
            <th scope="col">Count</th>
            <th scope="col">Stock Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model.UsedInventory)
        {
            @Html.Raw("<tr><td>")
            @Html.DisplayFor(m => i.Year)
            @Html.Raw("</td><td>")
            @Html.DisplayFor(m => i.Make)
            @Html.Raw("</td><td>")
            @Html.DisplayFor(m => i.Model)
            @Html.Raw("</td><td>")
            @Html.DisplayFor(m => i.Count)
            @Html.Raw("</td><td>")
            @Html.DisplayFor(m => i.StockValue)
            @Html.Raw("</td></tr>")
        }
    </tbody>
</table>
